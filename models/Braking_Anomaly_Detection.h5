   "source": [
    "# Step 4: Fit the Isolation Forest model\n",
    "isolation_forest = IsolationForest(contamination=0.1, random_state=42)\n",
    "isolation_forest.fit(scaled_features)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "5001dc31-3f9b-4f74-bbc5-2ad500e128d5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 5: Predict anomalies\n",
    "anomaly_scores = -isolation_forest.decision_function(scaled_features)  # Negative to align high scores with anomalies\n",
    "predicted_labels = isolation_forest.predict(scaled_features)\n",
    "# Convert predicted labels (-1 for anomaly, 1 for normal) to binary (1 for anomaly, 0 for normal)\n",
    "predicted_labels = (predicted_labels == -1).astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "742bfaa3-0e4a-47b6-8d0e-bca7185e929e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Add predictions and scores to the dataset\n",
    "braking_system_data['Anomaly Score'] = anomaly_scores\n",
    "braking_system_data['Predicted Label'] = predicted_labels"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "5d224b6a-c034-4bf2-9e8b-3a831698dcd9",
